// 32)​ Construir um algoritmo que calcule a média aritmética dos valores de um vetor de inteiros.

const vetorInteiros = [10, 20, 30, 40]

calculoMedia = vetor => {
    let somatoria = 0
    for(let i = 0; i < vetor.length; i++) {
       somatoria += vetor[i]
    }
    return (somatoria / vetor.length).toFixed(2)
}

console.log(calculoMedia(vetorInteiros))